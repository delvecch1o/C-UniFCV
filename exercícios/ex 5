#include <stdio.h>
#include <stdlib.h>

int main () {
	int valores[10];
	
	for (int i = 0; i < 10; i++) {
		printf("Digite um numero %d: ", i + 1);
		scanf("%d", &valores[i]);
	}
	
	int maior = valores[0], menor = valores[0];
	for (int i = 0; i < 10; i++) {
		if (valores[i] > maior) {
			maior = valores[i];
		}
		
		if (valores[i] < menor) {
			menor = valores[i];
		}
	}
	
	printf("Num maior: %d\n", maior);
	printf("Num menor: %d\n", menor);
}
